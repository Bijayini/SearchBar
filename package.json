{
  "name": "SearchBar",
  "private": true,
  "version": "1",
  "description": "",
  "main": "./src/index.js",
  "author": "Bijayini Parhi",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles --no-cache --config ./jest.config.js",
    "test:watch": "jest --watch",
    "update-snapshot": "jest --u",
    "coverage": "yarn run test -- --coverage && open ./coverage/lcov-report/index.html",
    "lint:scripts": "eslint \"src/**/*.{js,jsx}\"",
    "format": "yarn run lint:scripts --fix"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.3.0",
    "@testing-library/react-hooks": "^3.3.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "css-loader": "^3.6.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.12.4",
    "extract-text-webpack-plugin": "next",
    "html-webpack-plugin": "^4.3.0",
    "husky": "2.2.0",
    "jest": "^26.0.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "8.1.6",
    "node-sass": "^7.0.0",
    "prettier": "1.17.1",
    "prop-types": "15.7.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^6.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  }
}
